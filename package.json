{
  "name": "repairflow",
  "version": "1.7.6",
  "description": "A comprehensive, open-source repair shop management system",
  "author": "RepairFlow Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cranknet/repairflow.git"
  },
  "bugs": {
    "url": "https://github.com/cranknet/repairflow/issues"
  },
  "homepage": "https://github.com/cranknet/repairflow#readme",
  "keywords": [
    "repair-shop",
    "management-system",
    "nextjs",
    "typescript",
    "prisma",
    "open-source"
  ],
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev",
    "dev:web": "next dev",
    "build": "prisma generate && next build",
    "build:web": "prisma generate && next build",
    "start": "next start",
    "start:web": "next start",
    "lint": "eslint .",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "tsx scripts/reset-data.ts",
    "reset-admin-password": "tsx scripts/reset-admin-password.ts",
    "version:patch": "node scripts/update-version.js patch",
    "version:minor": "node scripts/update-version.js minor",
    "version:major": "node scripts/update-version.js major",
    "version:set": "node scripts/update-version.js set",
    "dev:electron": "concurrently \"cross-env BROWSER=none next dev\" \"wait-on http://localhost:3000 && electron .\"",
    "build:electron": "prisma generate && next build",
    "electron:build": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false npm run build:electron && electron-builder --win",
    "electron:dist": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false npm run build:electron && electron-builder --win --publish never"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.0",
    "@heroicons/react": "^2.1.1",
    "@hookform/resolvers": "^3.3.2",
    "@material/material-color-utilities": "^0.3.0",
    "@material/web": "^2.4.1",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@serialport/parser-readline": "^12.0.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "date-fns": "^3.0.6",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.344.0",
    "material-symbols": "^0.40.0",
    "nanoid": "^5.0.4",
    "next": "^16.0.5",
    "next-auth": "^5.0.0-beta.4",
    "nodemailer": "^7.0.11",
    "qrcode.react": "^3.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.49.2",
    "react-image-crop": "^10.1.8",
    "react-to-print": "^2.14.15",
    "recharts": "^3.5.0",
    "serialport": "^12.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.16",
    "baseline-browser-mapping": "^2.8.32",
    "clsx": "^2.0.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.5",
    "postcss": "^8.4.32",
    "prisma": "^5.7.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "wait-on": "^9.0.3"
  }
}